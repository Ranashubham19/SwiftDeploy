# --- SWIFTDEPLOY AI (OPENROUTER-ONLY) ---

NODE_ENV=production

# URLs
BASE_URL=https://your-railway-app.up.railway.app
FRONTEND_URL=https://your-frontend-domain.vercel.app
VITE_API_URL=https://your-railway-app.up.railway.app
BACKEND_API_URL=https://your-railway-app.up.railway.app

# Auth
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET_HERE
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE
GOOGLE_CALLBACK_URL=https://your-railway-app.up.railway.app/auth/google/callback
SESSION_SECRET=YOUR_VERY_LONG_RANDOM_SESSION_SECRET_HERE_MIN_32_CHARS
JWT_SECRET=YOUR_VERY_LONG_RANDOM_JWT_SECRET_HERE_MIN_32_CHARS

# Telegram
TELEGRAM_BOT_TOKEN=YOUR_PRIMARY_TELEGRAM_BOT_TOKEN
SINGLE_TELEGRAM_TOKEN_ONLY=true

# AI (OpenRouter only)
AI_PROVIDER=openrouter
AI_FORCE_PROVIDER=true
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY_HERE
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1/chat/completions
DEFAULT_MODEL=openrouter/free
OPENROUTER_MODEL=openrouter/free

# Optional OpenRouter model pools (keep OpenRouter ids only)
OPENROUTER_MODELS=
OPENROUTER_MODELS_GENERAL=
OPENROUTER_MODELS_CODING=
OPENROUTER_MODELS_MATH=
OPENROUTER_MODELS_REALTIME=

# Server
PORT=3001

# Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
EMAIL_FROM=your_email@gmail.com

# Discord (optional)
DISCORD_TOKEN=
DISCORD_CLIENT_ID=
DISCORD_GUILD_ID=
